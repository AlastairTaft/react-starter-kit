{
  "name": "simple-react-starter-kit",
  "private": true,
  "version": "0.0.0",
  "description": "A simple react starter kit",
  "main": "lib/server.js",
  "dependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.13.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-look": "^1.0.0-beta8",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redux": "^3.5.2",
    "redux-async-props": "^0.1.10",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "chalk": "^1.1.3",
    "gaze": "^1.0.0",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d lib && npm run build:tools && node tools-es5/build.js",
    "build:source-maps": "rimraf lib && babel src -d lib --source-maps",
    "build:tools": "rimraf tools-es5 && babel tools -d tools-es5",
    "start": "npm run build:tools && npm run build:source-maps && node tools-es5/start.js"
  },
