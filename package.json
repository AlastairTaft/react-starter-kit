{
  "name": "simple-react-starter-kit",
  "private": true,
  "version": "0.0.0",
  "description": "A simple react starter kit",
  "main": "dist/server.bundle.js",
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "compression-webpack-plugin": "^0.3.2",
    "connect-gzip-static": "^1.0.0",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "jss": "^5.5.2",
    "jss-preset-default": "^0.3.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-tap-event-plugin": "*",
    "redux": "^3.5.2",
    "redux-async-props": "^0.1.10",
    "rimraf": "^2.5.4",
    "serviceworker-webpack-plugin": "^0.1.7",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-hot-middleware": "^2.14.0"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "gaze": "^1.0.0",
    "source-map-support": "^0.4.0",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "start": "npm run build:clean && npm run build:copyBaseFiles && npm run build:emptyStaticFiles && NODE_PATH=./node_modules node server.js",
    "build:clean": "rimraf docs",
    "build:copyBaseFiles": "mkdirp tools/baseFiles && ncp tools/baseFiles docs --stopOnErr",
    "build:emptyStaticFiles": "babel-node tools/buildStaticPages.js --shell",
    "build": "npm run build:clean && npm run build:copyBaseFiles & npm run build:bundle & npm run build:static",
    "build:bundle": "webpack --production",
    "build:static": "npm run build:static:templateRenderFunc && npm run build:static:pages",
    "build:static:templateRenderFunc": "rimraf tools/templateRender && webpack --config=webpack.templateRender.config.js",
    "build:static:pages": "babel-node tools/buildStaticPages.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlastairTaft/raincheck.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlastairTaft/raincheck/issues"
  },
  "homepage": "https://github.com/AlastairTaft/raincheck#readme"
}
